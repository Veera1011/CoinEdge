    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title><%= title %></title>
        <!-- Favicon -->
        <link rel="icon" type="image/x-icon" href="/favicon/favicon.ico">
        <link href="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css" rel="stylesheet">
        <link href="/css/sweetalert.css" rel="stylesheet">
        <!-- Font Awesome for icons -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet">
        <!-- Google Fonts -->
        <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&family=Space+Grotesk:wght@300;400;500;600;700&display=swap" rel="stylesheet">
        <!-- Custom CSS -->
        <link href="/css/style.css" rel="stylesheet">
        <link href="/css/popup.css" rel="stylesheet">
        <link href="/css/dashboard.css" rel="stylesheet">
        <link href="/css/portfolio.css" rel="stylesheet" >
        <link href="/css/loader.css" rel="stylesheet">
        <link href="/css/arbitrage.css" rel="stylesheet">
        <link href="/css/settings.css" rel="stylesheet">
        <link href="/css/layout.css" rel="stylesheet">
        <link href="/css/deposit.css" rel="stylesheet">
        <link href="/css/withdraw.css"  rel="stylesheet">
        <link href="/css/loans.css"  rel="stylesheet">
        <link href="/css/table.css"  rel="stylesheet">
        <link  href="/css/customercare.css" rel="stylesheet">
    </head>
    <body>
        <!-- Header -->
        <header class="navbar">
            <div class="container-fluid">
                <div class="navbar-content">
                    <!-- Brand/Logo -->
                    <div class="navbar-brand">
                        <div class="logo-container">
                            <img src="/assets/logo/CoinEdgeLogo.png" alt="CoinEdge Logo" class="logo-img">
                        </div>
                        <span class="brand-text" id="loginSpanpic"></span>
                        <span class="brand-text" id="loginSpan"></span>
                        <span class="brand-text" id="loginSpanname"></span>
                    </div>
                    
                    <!-- Login Button -->
                    <div class="navbar-actions">
                        <a href="/auth/login" class="login-btn" id="loginBtn">
                        </a>
                    </div>
                </div>
            </div>
        </header>
        <!-- Main Content -->
        <main class="main-content">
            <%- body %>
        </main>

        <!-- Footer -->
        <footer>
            <div class="footer-container">
                <div class="footer-content">
                    <a class="footer-icon disabled" title="Home Dashboard" onclick="authCondition('homeBtn')" id="homeBtn">
                        <img src="/assets/footer/home.png" alt="Home" class="footer-gif">
                    </a>
                    <a class="footer-icon disabled" title="Market Data" onclick="authCondition('marketBtn')" id="marketBtn">
                        <img src="/assets/footer/market.png" alt="Market" class="footer-gif">
                    </a>
                    <a class="footer-icon disabled" title="My Portfolio" onclick="authCondition('portfolioBtn')" id="portfolioBtn">
                        <img src="/assets/footer/portfolio.png" alt="Portfolio" class="footer-gif">
                    </a>
                    <a class="footer-icon disabled" title="Trading With Us" onclick="authCondition('tradingBtn')" id="tradingBtn">
                        <img src="/assets/footer/trading.png" alt="Trading" class="footer-gif">
                    </a>
                    <a class="footer-icon disabled" title="Account Settings" onclick="authCondition('settingsBtn')" id="settingsBtn">
                        <img src="/assets/footer/setting.png" alt="Settings" class="footer-gif">
                    </a>
                </div>
            </div>
        </footer>

            <!-- Simple Popup with Font Awesome Icons -->
            <div id="popupOverlay" class="popup-overlay" style="display: none;">
                <div class="popup">
                    <i class="popup-icon" id="popupIcon"></i>
                    <div class="popup-title" id="popupTitle"></div>
                    <div class="popup-message" id="popupMessage"></div>
                    <button class="popup-button" id="popupButton">OK</button>
                </div>
            </div>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

        <!-- Custom JS -->
 
           
        <script src="/js/user-data.js"></script>
   
        <% if (typeof page !== 'undefined' && page === 'index') { %>
        <script src="/js/index.js"></script>
        <% } %>
         <script src="/js/main.js"></script>
        
        <script src="/js/chart.js"></script>
        <script src="/js/realtime.js"></script>
      
        <% if (typeof page !== 'undefined' && page === 'index') { %>
          <script src="/js/withdraw.js"></script>
        <% } %>
      

    </body>
    </html>
